pre.vue-container
  border-color #42b983
  border-left-width 0.5rem
  border-left-style solid
  border-radius 0

  & > code
    font-size 14px !important

    & > p
      margin -5px 0 -20px

    code
      padding 3px 5px
      color #000
      background-color #42b983 !important
      border-radius 3px

    em
      color #808080
      font-weight light

.box
  position relative
  padding 1px
  color rgb(99 209 173)
